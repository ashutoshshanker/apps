{
  "ethernet": {
    "type" : "object",
    "properties" : {
      "prompt" : {
        "type": "string",
        "default" : "eth"
      },
      "cliname" : {
        "type" : "string",
        "description" : "name used in cli to describe ethernet",
        "default" : "ethernet"
      },
      "value" : {
        "type" : "integer",
        "description" : "this is a trigger to cli that a command needs a value"
      },
      "help": {
        "type" : "string",
        "default": "Ethernet IEEE 802.3z"
      },
      "commands" : {
        "type": "object",
        "description": "holds all related commands related to ethernet port",
        "properties": {
          "subcmd1": {
            "$ref": "file:/tmp/snaproute/cli/schema/PortMembers.json"
          },
          "subcmd2": {
            "$ref": "file:/tmp/snaproute/cli/schema/ip.json"
          }
        }
      }
    },
    "required" : [
      "cliname",
      "commands"
    ]
  }
}
